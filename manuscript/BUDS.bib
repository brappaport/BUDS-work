@article{hillTemporalDynamicsEmotional,
  title = {Temporal Dynamics of Emotional Processing: {{Parsing}} Trial-Wise Variance of the Late Positive Potential Using {{Generalizability Theory}}},
  shorttitle = {Temporal Dynamics of Emotional Processing},
  author = {Hill, Kaylin E. and Haney, Alison M. and Foti, Dan and Aslinger, Elizabeth and Thomas, Katherine M. and Lane, Sean P.},
  journal = {Psychophysiology},
  volume = {n/a},
