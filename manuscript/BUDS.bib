@article{chandrakumarEventRelatedPotentialsRelation2018,
  title = {Event-{{Related Potentials}} in {{Relation}} to {{Risk-Taking}}: {{A Systematic Review}}},
  shorttitle = {Event-{{Related Potentials}} in {{Relation}} to {{Risk-Taking}}},
  author = {Chandrakumar, Dilushi and Feuerriegel, Daniel and Bode, Stefan and Grech, Megan and Keage, Hannah A. D.},
  year = {2018},
