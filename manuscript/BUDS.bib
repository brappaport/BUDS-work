@article{bridwellMovingERPComponents2018,
  title = {Moving {{Beyond ERP Components}}: {{A Selective Review}} of {{Approaches}} to {{Integrate EEG}} and {{Behavior}}},
  shorttitle = {Moving {{Beyond ERP Components}}},
  author = {Bridwell, David A. and Cavanagh, James F. and Collins, Anne G. E. and Nunez, Michael D. and Srinivasan, Ramesh and Stober, Sebastian and Calhoun, Vince D.},
