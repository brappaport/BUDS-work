@article{wuBetterDevilYou2021,
  title = {Better the Devil You Know than the Devil You Don't: {{Neural}} Processing of Risk and Ambiguity},
  shorttitle = {Better the Devil You Know than the Devil You Don't},
  author = {Wu, Shuyi and Sun, Sai and Camilleri, Julia A. and Eickhoff, Simon B. and Yu, Rongjun},
  year = {2021},
  month = aug,
  journal = {NeuroImage},
  volume = {236},
  pages = {118109},
  issn = {1053-8119},
  doi = {10.1016/j.neuroimage.2021.118109},
  urldate = {2023-05-24},
  abstract = {Risk and ambiguity are inherent in virtually all human decision-making. Risk refers to a situation in which we know the precise probability of potential outcomes of each option, whereas ambiguity refers to a situation in which outcome probabilities are not known. A large body of research has shown that individuals prefer known risks to ambiguity, a phenomenon known as ambiguity aversion. One heated debate concerns whether risky and ambiguous decisions rely on the same or distinct neural circuits. In the current meta-analyses, we integrated the results of neuroimaging research on decision-making under risk (n~=~69) and ambiguity (n~=~31). Our results showed that both processing of risk and ambiguity showed convergence in anterior insula, indicating a key role of anterior insula in encoding uncertainty. Risk additionally engaged dorsomedial prefrontal cortex (dmPFC) and ventral striatum, whereas ambiguity specifically recruited the dorsolateral prefrontal cortex (dlPFC), inferior parietal lobe (IPL) and right anterior insula. Our findings demonstrate overlapping and distinct neural substrates underlying different types of uncertainty, guiding future neuroimaging research on risk-taking and ambiguity aversion.},
  langid = {english},
  keywords = {ALE,Ambiguity,Meta-analysis,Neuroimaging,Reward,Risk,Uncertainty},
  file = {/Users/brentrappaport/Zotero/storage/WVZ9J55S/Wu et al. - 2021 - Better the devil you know than the devil you don't.pdf;/Users/brentrappaport/Zotero/storage/FQSMHKBZ/S1053811921003864.html}
}

