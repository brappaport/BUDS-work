@article{dammeResponsesPositiveAffect2022,
  title = {Responses to Positive Affect and Unique Resting-State Connectivity in Individuals at Clinical High-Risk for Psychosis},
  author = {Damme, Katherine S.F. and Gupta, Tina and Haase, Claudia M. and Mittal, Vijay A.},
  year = {2022},
  journal = {NeuroImage: Clinical},
