@article{olatunjiLongitudinalTraitstateModel2021,
  title = {A Longitudinal Trait-State Model of Attentional Control: {{Implications}} for Repetitive Negative Thinking},
  shorttitle = {A Longitudinal Trait-State Model of Attentional Control},
