@article{al-dajaniPsychometricReviewPersonality2016,
  title = {A {{Psychometric Review}} of the {{Personality Inventory}} for {{DSM}}--5 ({{PID}}--5): {{Current Status}} and {{Future Directions}}},
  shorttitle = {A {{Psychometric Review}} of the {{Personality Inventory}} for {{DSM}}--5 ({{PID}}--5)},
  author = {{Al-Dajani}, Nadia and Gralnick, Tara M. and Bagby, R. Michael},
