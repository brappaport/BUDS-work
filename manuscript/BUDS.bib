@article{conleyDevelopmentUncertainContexts2023,
  title = {Development in Uncertain Contexts: {{An}} Ecologically Informed Approach to Understanding Decision-Making during Adolescence},
  shorttitle = {Development in Uncertain Contexts},
  author = {Conley, M. I. and {Baskin-Sommers}, A.},
  year = {2023},
