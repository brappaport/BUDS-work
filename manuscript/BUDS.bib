@article{weinbergNeuralResponsesSocial2021,
  title = {Neural Responses to Social Acceptance Predict Behavioral Adjustments Following Peer Feedback in the Context of a Real-Time Social Interaction Task},
  author = {Weinberg, Anna and Ethridge, Paige and Pegg, Samantha and Freeman, Clara and Kujawa, Autumn and Dirks, Melanie A.},
  year = {2021},
