  file = {/Users/brentrappaport/Zotero/storage/PQAR6BM2/Scharf et al. - 2022 - A tutorial on the use of temporal principal compon.pdf}
}

@article{schellHungerGamesAssociations2021,
  title = {Hunger Games: {{Associations}} between Core Eating Disorder Symptoms and Responses to Rejection by Peers during Competition},
