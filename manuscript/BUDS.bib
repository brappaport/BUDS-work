  shorttitle = {Assessing {{Lifetime Stress Exposure Using}} the {{Stress}} and {{Adversity Inventory}} for {{Adults}} ({{Adult STRAIN}})},
  author = {Slavich, George M. and Shields, Grant S.},
  year = {2018},
