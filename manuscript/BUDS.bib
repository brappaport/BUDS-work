}

@article{pushkarskayaNeuralCorrelatesDecisionMaking2015,
  title = {Neural {{Correlates}} of {{Decision-Making Under Ambiguity}} and {{Conflict}}},
  author = {Pushkarskaya, Helen and Smithson, Michael and Joseph, Jane E. and Corbly, Christine and Levy, Ifat},
