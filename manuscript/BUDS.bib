@article{cano-lopezEffectsAcuteStress2016,
  title = {Effects of {{Acute Stress}} on {{Decision Making}} under {{Ambiguous}} and {{Risky Conditions}} in {{Healthy Young Men}}},
  author = {{Cano-L{\'o}pez}, Irene and {Cano-L{\'o}pez}, Beatriz and Hidalgo, Vanesa and {Gonz{\'a}lez-Bono}, Esperanza},
