@article{smithLowerLevelsDirected2022,
  title = {Lower {{Levels}} of {{Directed Exploration}} and {{Reflective Thinking Are Associated With Greater Anxiety}} and {{Depression}}},
  author = {Smith, Ryan and Taylor, Samuel and Wilson, Robert C. and Chuning, Anne E. and Persich, Michelle R. and Wang, Siyu and Killgore, William D. S.},
