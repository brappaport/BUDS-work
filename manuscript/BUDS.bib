}

@article{rios-salasPerceivedDiscriminationSocioeconomic2015,
  title = {Perceived Discrimination, Socioeconomic Status, and Mental Health among {{Latino}} Adolescents in {{US}} Immigrant Families},
  author = {{R{\'i}os-Salas}, Vanessa and Larson, Andrea},
