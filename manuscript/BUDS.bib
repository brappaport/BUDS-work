@article{tanakaAreAmbiguityAversion2015,
  title = {Are Ambiguity Aversion and Ambiguity Intolerance Identical? {{A}} Neuroeconomics Investigation},
  shorttitle = {Are Ambiguity Aversion and Ambiguity Intolerance Identical?},
  author = {Tanaka, Yusuke and Fujino, Junya and Ideno, Takashi and Okubo, Shigetaka and Takemura, Kazuhisa and Miyata, Jun and Kawada, Ryosaku and Fujimoto, Shinsuke and Kubota, Manabu and Sasamoto, Akihiko and Hirose, Kimito and Takeuchi, Hideaki and Fukuyama, Hidenao and Murai, Toshiya and Takahashi, Hidehiko},
