  author = {Haller, Simone P. and Chen, Gang and Kitt, Elizabeth R. and Smith, Ashley R. and Stoddard, Joel and Abend, Rany and Cardenas, Sofia I. and Revzina, Olga and Coppersmith, Daniel and Leibenluft, Ellen and Brotman, Melissa A. and Pine, Daniel S. and Pagliaccio, David},
  year = {2022},
