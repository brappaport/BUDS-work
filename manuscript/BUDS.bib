@article{netaImpactUncertainThreat2017,
  title = {The Impact of Uncertain Threat on Affective Bias: {{Individual}} Differences in Response to Ambiguity.},
  shorttitle = {The Impact of Uncertain Threat on Affective Bias},
  author = {Neta, Maital and Cantelon, Julie and Haga, Zachary and Mahoney, Caroline R. and Taylor, Holly A. and Davis, F. Caroline},
  year = {2017},
  month = dec,
  journal = {Emotion},
  volume = {17},
