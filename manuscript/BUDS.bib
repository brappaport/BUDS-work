@article{bernsteinSingleSessionWorkshopEnhance2021,
  title = {A {{Single-Session Workshop}} to {{Enhance Emotional Awareness}} and {{Emotion Regulation}} for {{Graduate Students}}: {{A Pilot Study}}},
