@article{nelsonIntoleranceUncertaintyMediates2014,
  title = {Intolerance of Uncertainty Mediates Reduced Reward Anticipation in Major Depressive Disorder},
  author = {Nelson, Brady D. and Shankman, Stewart A. and Proudfit, Greg H.},
