@article{deyoungDistinctionSymptomsTraits2022,
  title = {The Distinction between Symptoms and Traits in the {{Hierarchical Taxonomy}} of {{Psychopathology}} ({{HiTOP}})},
  author = {DeYoung, Colin G. and Chmielewski, Michael and Clark, Lee Anna and Condon, David M. and Kotov, Roman and Krueger, Robert F. and Lynam, Donald R. and Markon, Kristian E. and Miller, Joshua D. and {Mullins-Sweatt}, Stephanie N. and Samuel, Douglas B. and Sellbom, Martin and South, Susan C. and Thomas, Katherine M. and Watson, David and Watts, Ashley L. and Widiger, Thomas A. and Wright, Aidan G. C. and {HiTOP Normal Personality Workgroup}},
  year = {2022},
