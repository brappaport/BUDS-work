@article{dhingraEffectsAgeReward2020,
  title = {The Effects of Age on Reward Magnitude Processing in the Monetary Incentive Delay Task},
  author = {Dhingra, Isha and Zhang, Sheng and Zhornitsky, Simon and Le, Thang M. and Wang, Wuyi and Chao, Herta H. and Levy, Ifat and Li, Chiang-Shan R.},
