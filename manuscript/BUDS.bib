  pmid = {28138418},
  keywords = {anterior cingulate cortex,anticipatory anxiety,panic disorder,unpredictable threat},
  file = {/Users/brentrappaport/Zotero/storage/VMFS66VZ/Lieberman et al. - 2017 - Impact of Panic on Psychophysiological and Neural .pdf}
