@article{carletonIntoleranceUncertaintyContributor2013,
  title = {Intolerance of {{Uncertainty}} as a {{Contributor}} to {{Fear}} and {{Avoidance Symptoms}} of {{Panic Attacks}}},
  author = {Carleton, R. Nicholas and Fetzner, Mathew G. and Hackl, Jennifer L. and McEvoy, Peter},
