@article{simmonsEffectsTemporalUnpredictability2013,
  title = {The Effects of Temporal Unpredictability in Anticipation of Negative Events in Combat Veterans with {{PTSD}}},
  author = {Simmons, Alan N. and Flagan, Taru M. and Wittmann, Marc and Strigo, Irina A. and Matthews, Scott C. and Donovan, Heather and Lohr, James B. and Paulus, Martin P.},
  year = {2013},
