@article{gorkaEmotionbasedBrainMechanisms2019,
  title = {Emotion-Based Brain Mechanisms and Predictors for {{SSRI}} and {{CBT}} Treatment of Anxiety and Depression: A Randomized Trial},
  shorttitle = {Emotion-Based Brain Mechanisms and Predictors for {{SSRI}} and {{CBT}} Treatment of Anxiety and Depression},
  author = {Gorka, Stephanie M. and Young, Christina B. and Klumpp, Heide and Kennedy, Amy E. and Francis, Jennifer and Ajilore, Olusola and Langenecker, Scott A. and Shankman, Stewart A. and Craske, Michelle G. and Stein, Murray B. and Phan, K. Luan},
