@article{donaldsonTemporalDynamicsReversal2016,
  title = {The Temporal Dynamics of Reversal Learning: {{P3}} Amplitude Predicts Valence-Specific Behavioral Adjustment},
  shorttitle = {The Temporal Dynamics of Reversal Learning},
  author = {Donaldson, Kayla R. and Ait Oumeziane, Belel and H{\'e}lie, Sebastien and Foti, Dan},
  year = {2016},
  month = jul,
  journal = {Physiology \& Behavior},
  volume = {161},
  pages = {24--32},
  issn = {0031-9384},
  doi = {10.1016/j.physbeh.2016.03.034},
  urldate = {2024-01-02},
  abstract = {Adapting behavior to dynamic stimulus-reward contingences is a core feature of reversal learning and a capacity thought to be critical to socio-emotional behavior. Impairment in reversal learning has been linked to multiple psychiatric outcomes, including depression, Parkinson's disorder, and substance abuse. A recent influential study introduced an innovative laboratory reversal-learning paradigm capable of disentangling the roles of feedback valence and expectancy. Here, we sought to use this paradigm in order to examine the time-course of reward and punishment learning using event-related potentials among a large, representative sample (N=101). Three distinct phases of processing were examined: initial feedback evaluation (reward positivity, or RewP), allocation of attention (P3), and sustained processing (late positive potential, or LPP). Results indicate a differential pattern of valence and expectancy across these processing stages: the RewP was uniquely related to valence (i.e., positive vs. negative feedback), the P3 was uniquely associated with expectancy (i.e., unexpected vs. expected feedback), and the LPP was sensitive to both valence and expectancy (i.e., main effects of each, but no interaction). The link between ERP amplitudes and behavioral performance was strongest for the P3, and this association was valence-specific. Overall, these findings highlight the potential utility of the P3 as a neural marker for feedback processing in reversal-based learning and establish a foundation for future research in clinical populations.},
  keywords = {ERP,Flexibility,Punishment,Reversal-learning,Reward},
  file = {/Users/brentrappaport/Zotero/storage/RBZ7R5W9/Donaldson et al. - 2016 - The temporal dynamics of reversal learning P3 amp.pdf;/Users/brentrappaport/Zotero/storage/V55NATTL/S0031938416301317.html}
}

@article{dongIndividualDifferencesWorking2015,
