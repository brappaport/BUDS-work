  keywords = {Adaptation Psychological,Adolescent,Adult,Affect,Cerebral Cortex,Facial Expression,Female,Functional Laterality,Humans,Image Processing Computer-Assisted,Magnetic Resonance Imaging,Male,Neuropsychological Tests,Oxygen,Photic Stimulation,Uncertainty},
