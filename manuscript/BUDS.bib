@article{correaEventrelatedPotentialsAcoustic2022,
  title = {Event-Related Potentials to Acoustic Startle Probes during Unpredictable Threat Are Associated with Individual Differences in Intolerance of Uncertainty},
  author = {Correa, Kelly A. and Li, Lilian Y. and Nelson, Brady D. and Shankman, Stewart A.},
